      <% end %>
      <%= link_to csv_fine_tune_path(csv_fine_tune), data: { turbo_method: :delete, turbo_confirm: "Are you sure? You can't undo this.", turbo_frame: :_top }, class: "btn rounded-full p-2" do %>
        <span class="sr-only">Delete</span>
        <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 20 20" fill="currentColor" class="w-4 h-4">
