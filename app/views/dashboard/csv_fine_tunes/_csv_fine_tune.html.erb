<%= render('components/tables/tr', options: { id: dom_id(csv_fine_tune) }) do %>
  <%= render 'components/tables/td' do %>
    <div class="space-y-1">
      <p class="text-tertiary-900"><%= csv_fine_tune.name %></p>
      <p class="text-xs"><%= time_ago_in_words(csv_fine_tune.created_at) %> ago</p>
    </div>
  <% end %>
  <%= render 'components/tables/td' do %>
    <%= csv_fine_tune.description %>
  <% end %>
